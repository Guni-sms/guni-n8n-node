{
  "name": "n8n-nodes-guni",
  "version": "1.0.3",
  "description": "n8n community node for sending SMS and MMS using Guni API",
  "keywords": [
    "n8n-community-node-package",
    "guni",
    "sms",
    "mms"
  ],
  "license": "MIT",
  "homepage": "https://bitbucket.org/guni-sms/n8n-app",
  "author": {
    "name": "Gunisms",
    "email": "support@guni.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/guni-sms/n8n-app.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GuniApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Guni/Guni.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "merge-stream": "^2.0.0",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
